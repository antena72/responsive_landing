                padding-left em
                position relative
                text-transform uppercase
                span 
                    animation marquee 15s linear infinite
                    display inline-block
                    padding-left 100%
                    text-indent 0
                    &hover
                        animation-play-state paused
                
                buton.slick-prev.slick-arrow
                    color gris_clarito
      
        #formularioSolicitud  
