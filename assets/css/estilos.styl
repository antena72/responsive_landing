.boxSeccion
    display block
    clear both
    margin 0 auto
    min-height 900px
    max-width 1020px
    overflow hidden
    padding 50px 2%
    width 96%
/* On screens that are 992px wide or less, go from four columns to two columns */
@media (max-width: 768px) //tablets
    #miHeader 
        .mobile-toggle
            display initial
        #navHeader
            display none
            padding 0
            & ul li a
                display none
            &.open-nav 
                background-color white
                display block
                width 100%
                & ul li
                    margin 5px
                    width 100%
                    a
                        display initial
                        padding 10px
                        margin 0px
                        width 100%
    //cuerpo
    #miContenedor section #frase #franja h4                    
        width 80%
    #miContenedor section 
        #frase #presentacion
            margin 10px auto
            position relative
            width 100%
            #textos .bloquePresentacion
                margin 0 2%
                width 60%
            #imagen.bloquePresentacion
                display block
                margin 1%
                padding 10px
                position absolute
                top 20px
                right 20px
                vertical-align top
                width 30%
        h2
            margin 10px 20px
        #historia
            h2 
                margin-top 30px
            ul#numeros
                margin-left 6%
                margin-right 5%
                width 90%
                li
                    margin 1% 1% 1% 0
                    text-align center
                    width 31%
            #lineaTiempo
                margin-left 6%
                margin-right 5%
                width 90%
                #example
                    margin 0 auto
                    width 95%
        #centros
            margin 0 auto
            position relative
            width 100%
        #consultoria 
            margin 20px auto
            position relative
            width 100%
            #boxConsultoria
                margin-left 10px
                width 46%
            #formulario
                width 50%
                input[type=text]
                    width 100%
                    
    
        #novedades
            .boxSeccionNovedades #contenedorNovedades
                margin auto
                width 96%
                #galeriaNovedades
                    margin auto
                    width 96%
            .masInfo
                width 100%
                div
                    width 90%
    .boxSeccion
        margin 0 auto
        padding 0
        width 100%
@media (max-width: 375px) //mobile 
    #miContenedor
        section 
            h2 
                margin 0
                text-align center
            #inicio #barraGris 
                display block
                ul#headerRedes
                    margin-left 10px
                    padding 0
                    width 40%
                ul#circulitos
                    font-size 16px
                    margin-right 10px
                    width 50%
            
            #historia
                ul#numeros
                    margin-left 5%
                    width 96%
                    li 
                        width 45%
                        i
                            width 100%
            #centros 
                #menuCentros li:first-child
                    padding-right 0
                #menuCentros li a#centrosIntegrales
                #menuCentros li a#centrosServicios
                #menuCentros li:last-child
                    display none 
                #mapa
                    display none
                #referencias 
                    margin 50px 0 20px 20px
                    padding-top 0
                    width 80%
            #consultoria 
                #boxConsultoria
                    display none
                #formulario
                    margin 10px 2%
                    width 96%
            #novedades 
                #box2
                    background grey
                    margin 20px auto
                    width 60%
            #formularioSolicitud  
                #formulario2
                    height 450px
                    form
                        p.inputIcono
                            margin 1%
                            width 98%
                        p.inputArchivo
                            margin 10px 
                            & .explicacion 
                                font-size 10px
                                top 17px
                                left 40px 
                                i 
                                    padding-right 10px 
    
    footer  
        padding 20px 10px
        ul#bloquesFooter
            margin  0 auto
            width 96%
            li  
                width 49%
                li 
                    padding 0
                    margin 10px auto
                    width 100%
               
