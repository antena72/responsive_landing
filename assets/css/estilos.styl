                #detalle 
                    display none
                    padding 30px
                #botonDetalle i 
                    color gris_clarito
                    font-size 20px
                    padding 10px
            #barraGris
                background gris_clarito
                clear both
                display inline-block
                margin 0
                padding 10px 0
                width 100%
                ul 
                    li      
                        display inline
                #headerRedes
                    display inline-block
