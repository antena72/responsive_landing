verde = #B4BE00
gris_muy_oscuro = #1A1A1A
gris_oscuro = #555555
gris_medio = #666666
gris_9 = #999999
gris_clarito = #cccccc
grisBe = #bebebe
gris_F1 = #F1F1F1
gris_F9 = #F9F9F9
celeste = #66FEFD
naranja = #FB936D

*   
    margin 0
    padding 0
a 
    text-decoration none
ul 
    list-style-type none
body 
    font-family Arial, Helvetica, sans-serif
h1, h2, h3, h4, h5, h6 
    font-family Oswald, Arial
    font-weight normal
#miHeader
    background white
    border-bottom solid 4px gris_medio 
    display block
    margin 0
    overflow hidden
    position fixed
    padding 20px 0 5px
    -webkit-transition: all 0.3s;
    transition: all 0.3s;
    width 100%
    z-index 999
    #logo 
        display block
        float left
        margin 0
        padding 0 
        position relative
        text-align center
        width 25%
        a  
            color naranja
            font-family Oswald
            text-transform uppercase
            &:hover 
                color gris_medio
    #navHeader 
        display block
        float right
        margin 0
        padding 0
        position relative
        width 70%
        ul 
            display block
            margin 35px auto 0
            width 95%
            li 
                display inline-block
                padding 15px  
                a 
                    color gris_oscuro 
                    font-size 1em
                    text-align center
                    text-transform uppercase
                    &:hover 
                        color verde 
.mobile-toggle 
    display none
    cursor pointer
    position absolute
    right 22px
    top 30px
    width 30px
    -webkit-transition all 200ms ease-in
    -moz-transition all 200ms ease-in
    transition all 200ms ease-in

.open-nav .mobile-toggle 
    transform rotate(-90deg)
    -webkit-transform rotate(-90deg)
  
